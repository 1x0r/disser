  Timestamp                = {2015.11.30}
}

@Inproceedings{DivKazSof2013a,
  Title                    = {Synthesis of multicriterial intelligent control system using network operator method},
  Author                   = {Diveev, A.I. and Kazaryan, D.E. and Sofronova, E.A.},
  Booktitle                = {8th IEEE Conference on Industrial Electronics and Applications (ICIEA), 2013},
  Year                     = {2013},
  Month                    = jun,
  Pages                    = {331--336},

  Abstract                 = {In this work we consider an application of the network operator method for the intelligent control system synthesis. This system should guarantee object to meet several goals. If we weaken precision conditions, control system becomes optimal with respect to the general criterion. To solve the problem of synthesis the network operator method was used. This method allows searching for two multidimensional functions. The first function is responsible for the goal to be achieved, the second function is responsible for the goal selection area. We used the developed method to create the intelligent control system for flying robot that follows the three-dimensional path.},
  Doi                      = {10.1109/ICIEA.2013.6566390},
  Keywords                 = {aerospace robotics;control system synthesis;intelligent control;path planning;flying robot;goal selection area;multicriterial intelligent control system synthesis;multidimensional functions;network operator method;three-dimensional path;Conferences;Decision making;Indexes;Intelligent control;Switches;Vectors;Synthesis of control systems;flying robot;intellegent systems;quadrotor;the network operator method},
  Langid                   = {english},
  Language                 = {english},
  Owner                    = {David Kazaryan},
  Timestamp                = {2015.11.30}
}

