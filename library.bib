doi = {10.1109/CEC.2013.6557604},
file = {:C$\backslash$:/Users/David/Documents/Mendeley Desktop//Cerri et al. - 2013 - A grammatical evolution algorithm for generation of hierarchical multi-label classification rules.pdf:pdf},
journal = {Evolutionary Computation (CEC), 2013 IEEE Congress on},
pages = {454--461},
title = {{A grammatical evolution algorithm for generation of hierarchical multi-label classification rules}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84881573395{\&}partnerID=40{\&}md5=dfbae0f0f6fc70f91f80376f73e890f8},
