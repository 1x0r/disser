@article{Murphy2013,
abstract = {Grammars are an important tool for Evolutionary$\backslash$nComputation (EC). Grammars offer a flexible means of$\backslash$nsearch space restriction, and provide a mechanism for$\backslash$nimposing language and search biases. This thesis$\backslash$nexplores the use of a particular grammar type,$\backslash$nTree-Adjoining Grammars (TAGs) for use with Grammatical$\backslash$nEvolution (GE), a grammar-based EC algorithm. To date,$\backslash$nmuch of the work on GE has used Context-Free Grammars$\backslash$n(CFGs). TAGs have been shown to be more powerful than$\backslash$nCFGs and can generate some context-sensitive languages.$\backslash$nTAGs also exhibit interesting properties, such as, each$\backslash$nintermediate stage of TAG derivation being a fully$\backslash$nstructured feasible sentence from the language.$\backslash$n$\backslash$nThe focus of this thesis is to explore the use of TAGs$\backslash$nfor representation in GE. A definition of TAGs is given$\backslash$nand a comprehensive survey of TAGs in EC is presented.$\backslash$nFollowing this, a novel representation and mapping$\backslash$nprocess is developed which combines the linear$\backslash$nchromosome used by GE with TAGs. This extension of GE,$\backslash$ncalled Tree-Adjoining Grammatical Evolution (TAGE), is$\backslash$ncompared with canonical GE on a number of benchmark$\backslash$nproblems. TAGE demonstrates a performance benefit over$\backslash$nGE. Further study of the two representations is$\backslash$npresented, which identifies core representational$\backslash$ndifferences, such as, invalid individuals and neutral$\backslash$ncrossover operations, both of which do not occur in$\backslash$nTAGE. These differences are shown to account for some$\backslash$nof improved performance of TAGE.$\backslash$n$\backslash$nSubsequent to this, a novel method of rendering search$\backslash$nlandscapes is presented. Single mutation event$\backslash$nlandscapes are generated for GE and TAGE for a number$\backslash$nof common grammars. It is shown that TAGE search spaces$\backslash$nare much more densely connected than those of GE,$\backslash$naffording TAGE greater opportunities to move about the$\backslash$nsearch space.$\backslash$n$\backslash$nFurther representational differences in the form of$\backslash$npreferential language biases are discovered when$\backslash$ndeveloping methods of generating similar initial$\backslash$npopulations for both TAGE and GE. Two main biases are$\backslash$nidentified, adjunction constraints and grammar$\backslash$ntransformation biases. These biases affect the$\backslash$ndistributions of tree structures generated by TAGE.$\backslash$nMethods of mitigating these biases are presented and it$\backslash$nis shown that these biases can provide problem$\backslash$ndependent performance benefits.$\backslash$n$\backslash$nThe developmental nature of the feasibility property of$\backslash$nTAGs is exploited by integrating an on line artificial$\backslash$ngene regulatory network (GRN) model with TAGE in the$\backslash$nform of Developmental TAGE (DTAGE). DTAGE is shown to$\backslash$nimprove the usability of this GRN model by facilitating$\backslash$nit with the use of the TAGE mapping process. DTAGE is$\backslash$ndemonstrated to be capable of evolving GRNs whose$\backslash$noutput, when provided with feedback in the form of$\backslash$nstate information from dynamic problem environments,$\backslash$nmaps to phenotypes that can survive in those$\backslash$nenvironments.$\backslash$n$\backslash$nIn summary, this thesis explores the utility and$\backslash$ncapability of TAGs for representation in GE.$\backslash$nDifferences in representation between TAGs and CFGs for$\backslash$nuse with GE are identified and studied in terms of$\backslash$nperformance. TAGs are then exploited in the development$\backslash$nof a novel evolutionary developmental system combining$\backslash$nTAGs, GE and a GRN model.},
author = {Murphy, Eoin},
file = {:C$\backslash$:/Users/David/Documents/Mendeley Desktop/exploration{\_}of{\_}grammars{\_}in{\_}grammatical{\_}evolution.pdf:pdf},
title = {{An Exploration of Tree-Adjoining Grammars for Grammatical Evolution}},
year = {2013}
}
